      "keyword-spacing": "off",
      "comma-spacing": "off",
      "camelcase": "off",
      "spaced-comment": "off",
      "vue/return-in-computed-property": "off",
      "space-infix-ops": "off",
      "no-multiple-empty-lines": "off",
      "no-multi-spaces": "off",
      "prefer-const": "off",
      "eol-last": "off",
